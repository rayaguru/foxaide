

<!--  change modal-md to modal-lg for large modal ad modal-sm for small modals -->
<!--  using jquery add modal type classes to the id modalContainer -->


<div class="modal fade " id="msgSmModal" tabindex="-1" role="dialog" aria-labelledby="mySmallModalLabel" aria-hidden="true">
  <div id="modalContainer" class="modal-dialog modal-md">
    <div class="modal-content">
  				<div class="modal-header">
                    <button type="button" class="close" data-dismiss="modal" aria-hidden="true">&times;</button>
                    <h4 id="msgSmWinTitle" class="modal-title text-center"></h4>
                </div>
                
  		<h4 id="msgSmWinBody" class="text-center"></h4>
  
    </div>
  </div>
</div>
